{
	"plugins": [
        "react",
        "@typescript-eslint",
		"jest",
		"prettier"
		],
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"extends": [
        "eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"prettier/@typescript-eslint"
	],
	"rules": {
		"no-console": 0,
		"curly": 1,
		"no-case-declarations": 0,
		"no-irregular-whitespace": 0,
		"no-useless-return": 1,
		"no-unused-vars": 1,
		"block-spacing": 1,
		"no-empty": 1,
		"no-undef": 0
	}
}
